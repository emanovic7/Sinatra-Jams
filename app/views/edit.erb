<h1>Edit Artist</h1>


<form action="/artists/<%= @artist.id %>" method="POST">
  <input type="hidden" name="_method" value="PUT">

  <p>Name<input type="text" name="artist[name]" value="<%= @artist.name%>"></p>
  <p>Age<input type="text" name="artist[age]" value="<%= @artist.age%>"></p>
  <p>Bio<input type="text" name="artist[bio]" value="<%= @artist.bio%>"></p>

  <input type="Submit" value="Edit">
</form>
